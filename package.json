{
  "name": "gislayer-tile-server",
  "version": "1.0.0",
  "description": "this electron app for gislayer tile server",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=mac.icns --prune=true --out=build/mac",
    "build-win32": "electron-packager . gislayer --overwrite --platform=win32 --arch=ia32 --icon=favicon.ico --prune=true --out=build/windows32 --version-string.CompanyName=GISLayer --version-string.FileDescription=GISLayer --version-string.ProductName=\"GISLayer Local Tile Server\"",
    "build-win64": "electron-packager . gislayer --overwrite --platform=win32 --arch=x64 --icon=favicon.ico --prune=true --out=build/windows64 --version-string.CompanyName=GISLayer --version-string.FileDescription=GISLayer --version-string.ProductName=\"GISLayer Local Tile Server\"",
    "build-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=favicon.ico --prune=true --out=release-builds",
    "server": "node api/index.js"
  },
  "author": "ali kılıç",
  "license": "MIT",
  "devDependencies": {
    "electron": "^10.1.2",
    "electron-packager": "^15.1.0"
  },
  "dependencies": {
    "@mapbox/tile-cover": "^3.0.2",
    "@turf/bbox": "^6.0.1",
    "@turf/bbox-polygon": "^6.0.1",
    "@turf/boolean-contains": "^6.0.1",
    "@turf/turf": "^5.1.6",
    "ajv": "^6.12.5",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "canvas": "^2.11.2",
    "child_process": "^1.0.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "custom-electron-titlebar": "^3.2.4",
    "dxf-parser": "^1.0.0-alpha.2",
    "ejs": "^3.1.5",
    "epsg": "^0.5.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "geojson-vt": "^3.2.1",
    "geopackage": "^1.0.2",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nedb-promises": "^4.0.4",
    "proj4": "^2.6.3",
    "reproject": "^1.2.6",
    "semantic-ui-css": "^2.4.1",
    "shp": "0.0.1",
    "socket.io": "^3.0.3",
    "sqlite": "^3.0.6",
    "togeojson": "^0.16.0",
    "vt-pbf": "^3.1.1",
    "vue": "^2.6.12",
    "wkt": "^0.1.1",
    "xml": "^1.0.1",
    "xmldom": "^0.3.0"
  }
}
